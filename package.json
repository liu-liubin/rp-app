  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "win-start": "chcp 65001 && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "electron-forge make  ./ --platform=win32 --ignore=node_modules --arch=x64",
